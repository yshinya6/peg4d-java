TopLevel
    = Document

Chunk 
	= Document

File
	= Document
    

Document
    = { (@Prolog)? MISC* @Content MISC* #xmlDoc}
    

Prolog
    = { '<?xml' _* (@Attribute)* _* '?>' #prolog }
    

Content
    = _* { ((@Element0 / COMMENT) _*)* #content } _*
    

Name
    = { [A-Za-z:] ( '-' / [A-Za-z0-9:._])*  #name }

NAME
	= [A-Za-z:] ( '-' / [A-Za-z0-9:._])*

String
	= { '"' ( !'"' . )* '"' #string }

STRING
	= '"' ( !'"' . )* '"'

NMTOKEN
	= '"' NAME '"'

IDTOKEN
	= [A-Za-z0-9:._]+ / <isa IDLIST>


    
    
TEXT
    = (![<&] .)+
    


//Reference //TODO

Attribute
    = _* { @Name '=' @String #keyvalue }
    

Cdata
    = '<![CDATA['  { (!']]>' .)* #cdata } ']]>'
    


MISC
    = COMMENT / _+
    

COMMENT
    = '<!--' (!'-->' . )* '-->'
    
    
QM
		= '"'


VS
	= _* ',' _*
	
	
EMPTY
	= _*

Empty
	= {EMPTY #empty}

Any
	= {ANY #any}

ANY
	= _* TEXT _*
		

_ = [ \t\r\n];

